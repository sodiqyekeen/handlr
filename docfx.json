{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/**/*.csproj"
                    ],
                    "exclude": [
                        "**/bin/**",
                        "**/obj/**",
                        "**/packages/**",
                        "src/Handlr.SourceGenerator/**"
                    ]
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "properties": {
                "TargetFramework": "net9.0"
            },
            "filter": "filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "docs/**.md",
                    "*.md"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "_site/**"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "docs/toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "docs/images/**",
                    "images/**",
                    "assets/**",
                    "styles/**"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "_site/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "docs/overwrite/**/*.md"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [
            "ExtractSearchIndex"
        ],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "globalMetadata": {
            "_appTitle": "Handlr CQRS Framework",
            "_appName": "Handlr CQRS Framework",
            "_appFaviconPath": "assets/logos/handlr-favicon.svg",
            "_appLogoPath": "assets/logos/handlr-icon.svg",
            "_enableSearch": true,
            "_gitContribute": {
                "repo": "https://github.com/sodiqyekeen/handlr",
                "branch": "main"
            },
            "_gitUrlPattern": "github"
        }
    }
}